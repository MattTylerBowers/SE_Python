sefile = open('C:\\Users\\garci\\setext.txt', 'r')
fileContents = sefile.readlines()
blocks = int(fileContents[0].rstrip("\n"))
fileContents.pop(0)

#removing the \n on the readlines()
for x in range(500):
    fileContents[x] = fileContents[x].rstrip("\n")


